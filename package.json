{
  "name": "pocketrag",
  "version": "0.0.1",
  "description": "",
  "main": "./electron/main/main.js",
  "scripts": {
    "dev:vite": "vite",
    "dev": "concurrently -k \"npm run dev:vite\" \"electron .\"",
    "build:vite": "vite build",
    "build:kernel": "node ./scripts/build_kernel.js",
    "build:kernel:cuda": "node ./scripts/build_kernel.js --cuda",
    "build": "npm run build:vite && npm run build:kernel && electron-builder",
    "build:cuda" : "npm run build:vite && npm run build:kernel:cuda && electron-builder"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.1",
    "adm-zip": "^0.5.16",
    "axios": "^1.9.0",
    "concurrently": "^9.1.2",
    "electron": "^35.1.4",
    "electron-builder": "^26.0.12",
    "progress": "^2.0.3",
    "tar": "^7.4.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@bytemd/plugin-gfm": "^1.22.0",
    "@bytemd/plugin-highlight": "^1.22.0",
    "@bytemd/react": "^1.22.0",
    "antd": "^5.25.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.2"
  },
  "build": {
    "appId": "com.example.pocketrag",
    "files": [
      "electron/build/**/*",
      "electron/main/**/*"
    ],
    "extraResources": [
      {
        "from": "kernel/bin",
        "to": "bin"
      },
      {
        "from": "electron/public",
        "to": "public"
      }
    ],
    "directories": {
      "buildResources": "electron/resources",
      "output": "dist"
    },
    "extraMetadata": {
      "main": "electron/main/main.js"
    },
    "compression": "normal",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
